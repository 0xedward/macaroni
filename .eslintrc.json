{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": ["error", {
            "comments": 110,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
        }],
        "require-jsdoc" : ["off"]
    }
}
